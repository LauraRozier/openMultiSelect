{
    "env": {
        "es6": false,
        "browser": true,
        "node": false,
        "commonjs": true,
        "jquery": true
    },
    "extends": [ "eslint:recommended", "google" ],
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [ "error", "unix" ],
        "quotes": [ "error", "double" ],
        "semi": [ "error", "always" ]
    }
}
