{
  "name": "openmultiselect",
  "version": "1.0.0",
  "description": "Open Multi Select for MDBootstrap",
  "main": "src/js/openMultiSelect.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "npm run build-sass && npm run build-sass-min && npm run build-js",
    "build-sass": "cd css && sass openMultiSelect.scss:openMultiSelect.css",
    "build-sass-min": "cd css && sass openMultiSelect.scss:openMultiSelect.min.css --style=compressed",
    "build-js": "cd js && uglifyjs openMultiSelect.js -c -m -o openMultiSelect.min.js --config-file ../uglifyjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thibmo/openMultiSelect.git"
  },
  "keywords": [
    "mdbootstrap",
    "bootstrap",
    "bootstrap4",
    "jquery",
    "jquery-plugin",
    "multiselect",
    "multi-select"
  ],
  "author": "Thimo Braker <thibmorozier@live.nl>",
  "license": "Beerware",
  "bugs": {
    "url": "https://github.com/thibmo/openMultiSelect/issues"
  },
  "homepage": "https://github.com/thibmo/openMultiSelect#readme",
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-jquery": "^1.5.0",
    "babel-eslint": "^10.0.1",
    "uglify-es": "^3.3.9",
    "sass": "^1.16.0"
  }
}
